<?xml version="1.0" encoding="utf-8"?>
<com.open.lib.TouchView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/card"
    android:layout_width="300dp"
    android:layout_height="286dp"
    android:layout_marginBottom="10dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:background="@drawable/card_login_background_normal"
    android:visibility="visible" >

    <com.open.lib.TouchImageView
        android:id="@+id/grip_card_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/card_login_background_normal"
        android:visibility="invisible" />

    <include
        android:layout_width="match_parent"
        android:layout_height="286dp"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="25dp"
        layout="@layout/view_element_card" />

</com.open.lib.TouchView>